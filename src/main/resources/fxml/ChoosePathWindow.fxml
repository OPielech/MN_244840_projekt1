<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="80.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ib.ChoosePathController">
    <children>
        <Label layoutX="8.0" layoutY="27.0" prefHeight="30.0" prefWidth="50.0" text="Path:" textFill="WHITE">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="labelChoosePath" layoutX="64.0" layoutY="29.0" prefHeight="25.0" prefWidth="452.0"/>
        <Button fx:id="buttonApply" layoutX="522.0" layoutY="26.0" mnemonicParsing="false"
                onAction="#buttonApplyPressed" text="Apply" textFill="WHITE">
            <font>
                <Font name="System Bold" size="13.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
